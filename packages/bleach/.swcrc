{
  "$schema": "https://swc.rs/schema.json",
  "test": ".*.ts(x)?$",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true,
      "decorators": false
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "react": {
        "throwIfNamespace": true,
        "development": false,
        "useBuiltins": false,
        "runtime": "automatic"
      }
    },
    "target": "esnext",
    "loose": true,
    "keepClassNames": true,
    "preserveAllComments": true
  },
  "module": {
    "type": "es6",
    "strict": true,
    "strictMode": true,
    "lazy": false,
    "noInterop": false,
    "ignoreDynamic": false
  },
  "sourceMaps": true
}
