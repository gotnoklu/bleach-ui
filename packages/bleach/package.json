{
  "name": "bleach",
  "description": "The stylish UI library for React Native",
  "version": "0.0.0",
  "repository": "https://github.com/delalify/conjoin-monorepo",
  "author": "Enoch Klu <cyronix787@gmail.com>",
  "contributors": [
    {
      "name": "Delali Azumah",
      "url": "https://github.com/assetcorp"
    }
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "dev": "yarn build && chokidar \"./**/*.{ts,swcrc,json}\" -i \"./dist/**/*.{ts,json}\" -c \"yarn build\"",
    "build": "yarn lint && swc -d dist --sync --workers 2 --copy-files --delete-dir-on-start --strip-leading-paths --include-dotfiles src && tsc --declaration --emitDeclarationOnly",
    "lint": "biome lint && tsc --noEmit --skipLibCheck",
    "lint:fix": "biome lint --fix",
    "format": "biome format --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^18",
    "chokidar-cli": "^3.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-native": "^0.76.6",
    "react-native-reanimated": "^3.16.7",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": "^17",
    "react-dom": "^17",
    "react-native": "*",
    "react-native-reanimated": "*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4"
  }
}
